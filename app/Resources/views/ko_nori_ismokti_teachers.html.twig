{% extends 'default/base.html.twig' %}
{% block body %}
    <div class="col-md-4" style="border: 1px solid; margin-left: 37%; margin-right: 37%; margin-top: 5%">
        <div>
            <label class="col-md-12" style="text-align: center">Ko norite išmokti?</label>
            <label class="col-md-12" style="text-align: center">{{ sub_category.category }}</label>
        </div>
        <div>
            {% for value in canTeach %}

                <label class="col-md-12">{{ value.userId.username }}
                    <a class="btn btn-default" href="{{ path('fos_message_thread_new') }}" role="button">Rašyti žinutę</a></label>
            {% endfor %}
        </div>
    </div>
{% endblock %}